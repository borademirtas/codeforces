#include <iostream>
#include <vector>
#include <cstdio>
#include <algorithm>  
#include <fstream>
#include <string>
#include <cctype>
using namespace std;
#define ll long long 



int main() {
	ll n;
	cin >> n;
	if (n == 0) { cout << 1; return 0; }
	n %= 4;
	if (n == 0) { cout << 6; }
	if (n == 1) { cout << 8; }
	if (n == 2) { cout << 4; }
	if (n == 3) { cout << 2; }
	
}
