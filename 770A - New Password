#include <iostream>
#include <vector>
#include <cstdio>
#include <algorithm>  
#include <fstream>
#include <string>
using namespace std;
#define ll long long 



int main(){
	ll n, k, total = 0;
	char letter;
	cin >> n >> k;
	while (total < n) {
		for (ll i = 0; i < k; i++) {
			letter = ('a' + i);
			std::cout << letter;
			total++;
			if (total == n) { break; }
		}
	}
		
}
